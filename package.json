{
  "name": "profit-model-vite",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:gh": "vite build --base /Atom.Ak/",
    "build:vercel": "vite build",
    "build:zeabur": "vite build",
    "build:netlify": "vite build",
    "preview": "vite preview",
    "deploy:gh": "npm run build:gh && npx gh-pages -d dist",
    "deploy:vercel": "npm run build && vercel --prod",
    "deploy:zeabur": "npm run build && zeabur deploy",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=dist",
    "deploy:all": "npm run deploy:gh"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react-swc": "^3.7.0",
    "gh-pages": "^6.3.0",
    "vite": "^5.4.0"
  }
}
